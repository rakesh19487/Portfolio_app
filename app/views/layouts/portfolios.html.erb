<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%= @page_title %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'portfolios', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>

    <%= render 'shared/portfolios/portfolio_nav' %>
    <%= alerts %>
    <main role="main">
      <%= render 'shared/portfolios/portfolio_masthead' %>
      <%= yield %>
      <%#= render 'shared/portfolios/portfolio_footer' %>
      <%= session_source %>    
    </main>
  
  </body>
</html>
